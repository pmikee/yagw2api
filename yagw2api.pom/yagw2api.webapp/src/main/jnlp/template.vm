<?xml version="1.0" encoding="utf-8"?>
<jnlp
    spec="6.0+"
    codebase="$jnlpCodebase"
    href="$outputFile">

<information>
#if($project.name)
    <title>$project.name</title>
#end
#if($project.Organization.Name)
    <vendor>$project.Organization.Name</vendor>
#end
#if($project.Description)
    <description>$project.Description</description>
#end
#if($project.Url)
    <homepage href="$project.Url"/>
#end

	<shortcut online="true">
    	<desktop/>
    	<menu submenu="Yet another GW2 API - Sample Application"/>
  	</shortcut>
	
	<icon href="yagw2api_splash.jpg" kind="splash"/>
</information>
<update check="always" policy="prompt-update"/>
<security>
     <all-permissions/>
</security>

<resources>
    <j2se version="1.7+" href="http://java.com/de/download/index.jsp" initial-heap-size="256m" max-heap-size="1024m" />
    $dependencies
</resources>
<application-desc main-class="$mainClass"/>
</jnlp>